//
//      The Template Matrix Library for fundamental types
//      contains most of the operations on matrices.
//
//      The Template Matrix library is written in the C++20 standard
//      So for use you need at least GCC 10 or Clang 10
//
//      This file contains a StaticMatrix class that allows you
//      to work with a matrix on the stack.
//

#ifndef MATRIX_LIBRARY_CPP_STATIC_MATRIX_
#define MATRIX_LIBRARY_CPP_STATIC_MATRIX_

#if __cplusplus >= 202002L
    #include "implementation/static_matrix.h"
#endif

#endif //MATRIX_LIBRARY_CPP_STATIC_MATRIX_
